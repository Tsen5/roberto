{
  "name": "node-llama-cpp-project",
  "private": true,
  "version": "0.0.0",
  "main": "./dist-electron/index.js",
  "type": "module",
  "homepage": "https://github.com/withcatai/node-llama-cpp",
  "author": {
    "name": "Author name",
    "email": "email@example.com"
  },
  "scripts": {
    "postinstall": "npm run models:pull",
    "models:pull": "node-llama-cpp pull --dir ./models \"hf:bartowski/gemma-2-2b-it-GGUF:Q6_K_L\"",
    "start": "vite dev",
    "start:build": "electron ./dist-electron",
    "prebuild": "rimraf ./dist ./dist-electron ./release",
    "build": "tsc && vite build && electron-builder --config ./electron-builder.ts",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint --report-unused-disable-directives .",
    "format": "npm run lint:eslint -- --fix",
    "clean": "rm -rf ./node_modules ./dist ./dist-electron ./release ./models",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@floating-ui/react": "^0.27.15",
    "@fontsource-variable/inter": "^5.2.6",
    "@tanstack/react-router": "^1.130.12",
    "@tanstack/react-router-devtools": "^1.130.13",
    "birpc": "^2.5.0",
    "classnames": "^2.5.1",
    "color2k": "^2.0.3",
    "frimousse": "^0.3.0",
    "highlight.js": "^11.11.1",
    "i18next": "^25.3.2",
    "lifecycle-utils": "^3.0.1",
    "lucide-react": "^0.536.0",
    "markdown-it": "^14.1.0",
    "node-llama-cpp": "^3.11.0",
    "pdfreader": "^3.0.7",
    "pretty-ms": "^9.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.6.1",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "semver": "^7.7.2",
    "usehooks-ts": "^3.1.1",
    "uuid": "^11.1.0",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@stylistic/eslint-plugin": "^5.2.2",
    "@tanstack/router-plugin": "^1.130.15",
    "@types/markdown-it": "^14.1.2",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/semver": "^7.7.0",
    "@vitejs/plugin-react": "^4.7.0",
    "electron": "^37.2.5",
    "electron-builder": "^26.0.12",
    "eslint": "^9.32.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^52.0.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.4",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.0.6",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "zx": "^8.7.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{js,jsx,ts,tsx,json,md}": "prettier --write"
  },
  "overrides": {
    "electron-builder": {
      "read-config-file": {
        "config-file-ts": ">=0.2.8-rc1"
      }
    }
  }
}
